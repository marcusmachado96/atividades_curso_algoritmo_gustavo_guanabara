algoritmo "semnome"
var
   V1, V2, S: Inteiro
   
funcao soma (X, Y: Inteiro): Inteiro
inicio
      retorne X + Y
fimfuncao

inicio
      Escreval ("Digite o primeiro valor: ")
      Leia (V1)
      Escreval ("Digite o segundo valor: ")
      Leia (V2)
      S <- Soma (V1, V2)
      Escreval ("A soma entre", V1, " e ", V2, " e igual a ", S)
fimalgoritmo