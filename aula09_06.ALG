algoritmo "semnome"
var
   cont, n, m, p: Inteiro
inicio
      cont <- 1
      Enquanto (cont <= 10) faca
               p <- m
               Escreval ("Digite o valor ", cont, "o valor: ")
               Leia (n)
               m <- n + p
               cont <- cont + 1
      Fimenquanto
      Escreval ("A soma dos numeros é ", p)
fimalgoritmo